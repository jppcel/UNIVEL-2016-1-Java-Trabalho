package me.polles;


@Tabela("CLIENTE")
public class Cliente {
	
	@Coluna(pk=true)
	private int id;
	
	@Coluna(nome="NOME")
	private String nome;
	
	@Coluna(nome="ENDERECO")
	private String endereco;
	
	@Coluna(nome="TELEFONE")
	private String telefone;

	@Coluna(nome="CLESTADOCIVIL")
	private EstadoCivil estadoCivil;

}
