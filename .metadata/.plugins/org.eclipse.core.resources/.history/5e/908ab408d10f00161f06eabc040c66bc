package me.polles;

public enum EstadoCivil {
	
	SOLTEIRO("Solteiro", 1), 
	CASADO("Casado", 2),
	VIUVO("Viuvo", 3);
	
	private String estadoCivil;
	private final int id;
	
	private EstadoCivil(String estadoCiv, int idestado){
		this.estadoCiv = estadoCiv;
		this.idestado = idestado;
	}
	
	public String getEstadoCivil(){
		return estadoCivil;
	}
	
	public String getID(){
		return id;
	}

}
